<form action="/signup" method="post">

    <div class="container">
    <div class="mb-3 row">
      <label for="Email" class="col-sm-2 col-form-label">Email</label>
      <div class="col-sm-10">
        <input type="email" class="form-control" id="sEmail" name="semail">
      </div>
    </div>
    <div class="errorcode"><%=code %> </div>
    <div class="mb-3 row">
      <label for="Password" class="col-sm-2 col-form-label">Password</label>
      <div class="col-sm-10">
        <input type="password" class="form-control" id="sPassword" name="spassword">
      </div>
      <div class="mb-3 row">
        <label for="Passwordagain" class="col-sm-2 col-form-label">Confirm Password</label>
        <div class="col-sm-10">
          <input type="password" class="form-control" id="sPasswordagain" name="spasswordagain" onchange="checkmatch()">
        </div>

        <div class="warning" id="warning"></div>
    
    </div>
      <div class="loginbtn" id="buton">
        <button class="btn-primary btn-sm" type="submit">Login In</button>
      </div>
    </div>
    
    </form>

  